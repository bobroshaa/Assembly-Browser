<Window x:Class="Assembly_Browser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assembly_Browser"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid x:Name="Grid">
        <Button x:Name="ButtonChoose"  Width="100" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20 20" Content="Выбрать сборку" />
        <Button x:Name="ButtonClear"  Width="100" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20 60" Content="Очистить" />
        <TreeView x:Name="TreeView" Width="370" Height="310" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="20"></TreeView>
    </Grid>
</Window>
